# Google PageSpeed Insights API Key for REAL website analysis
# Get your API key from: https://developers.google.com/speed/docs/insights/v5/get-started
PAGESPEED_API_KEY=your_pagespeed_api_key_here

# Gemini AI API Key for performance analysis and recommendations
GEMINI_API_KEY=your_gemini_api_key_here

# Next.js Environment
NEXT_PUBLIC_APP_URL=https://your-domain.com
NEXT_PUBLIC_API_BASE_URL=http://localhost:8000

# PostHog Analytics Configuration
# Get your PostHog key from: https://app.posthog.com/project/settings
NEXT_PUBLIC_POSTHOG_KEY=your_posthog_project_api_key_here
NEXT_PUBLIC_POSTHOG_HOST=https://app.posthog.com

# PayPal Configuration
# Get your PayPal Client ID from: https://developer.paypal.com/dashboard/applications/sandbox
NEXT_PUBLIC_PAYPAL_CLIENT_ID=your_paypal_client_id_here