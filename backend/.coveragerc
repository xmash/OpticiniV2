[run]
source = .
omit = 
    */migrations/*
    */test_scripts/*
    */tests/*
    */venv/*
    */__pycache__/*
    manage.py
    */settings.py
    */conftest.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod

